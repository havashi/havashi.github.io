---
layout: none
---
[
  {% for post in site.drafts %}
    {
      "title"    : "{{ post.title | escape }}",
      {% if post.draftPublish %}"draftPublish"  : {{post.draftPublish}},{% endif%}
      {% assign cas = site.cases | where: 'slug', post.case | first %}{% if cas %}"case"   : "{{cas.title}}",{% endif %}
      "author"   : "{{post.guestEmail}}",
      "url"      : "{{ site.baseurl }}{{ post.url }}",
      "date"     : "{{post.editDate}}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]
