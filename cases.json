---
layout: none
---
[
  {% for post in site.cases %}
    {
      "title"    : "{{ post.title | escape }}",
      {% if post.draftPublish %}"draftPublish"  : {{post.draftPublish}},{% endif%}
      {% if post.guestEmail %}"author"   : "{{post.guestEmail}}",{% endif%}
      {% if post.editDate %}"date"     : "{{post.editDate}}",{% endif%}
      "url"      : "{{ site.baseurl }}{{ post.url }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]
